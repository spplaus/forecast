    "dotenv": "^8.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.7.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "css-loader": "^3.0.0",
    "dotenv-webpack": "^5.1.0",
